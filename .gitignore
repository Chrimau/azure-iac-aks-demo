#Ignore local .terraform directories
.terraform
.terraform.lock.hcl

#state sensitive files
*.tfstate
*.tfstate.backup
*.tfvars
*.tfstate.*   
# this rule above ignores every file with the tfstate extension

#crash logs
crash.logs
.logs

#override files
override.tf
override.tf.json
*_override.tf
*_override.tf.json

#Ignore sensitive variable files
*.auto.tfvars
*.tfvars
*.<YOUR_ENV>.tfvars
*.tfvars.json

#Ignore provider keys
*.pem
*.key
*.crt